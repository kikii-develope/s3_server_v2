{
  "name": "file_uploader",
  "version": "1.3.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:detach": "docker-compose -f docker-compose.dev.yml up -d --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:prod:detach": "docker-compose -f docker-compose.prod.yml up -d --build",
    "docker:stop:dev": "docker-compose -f docker-compose.dev.yml down",
    "docker:stop:prod": "docker-compose -f docker-compose.prod.yml down"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.817.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.1.4",
    "jsdoc": "^4.0.4",
    "multer": "^2.0.0",
    "mysql2": "^3.16.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "webdav": "^5.8.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.1",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.0.3",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3"
  },
  "author": "inseok lee",
  "license": "ISC"
}
